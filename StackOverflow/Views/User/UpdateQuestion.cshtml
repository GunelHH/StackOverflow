<script>let TagInput = document.getElementById("search-tag");
    let spanforTag = document.getElementById("tgspn");

    let removeTag = document.getElementById("tag-remove");

    removeTag.addEventListener("click", () => {
      console.log("deleted")
        spanforTag.value = "";
    })

        TagInput.addEventListener("input", (e) => {

            console.log("into");
            $.ajax({
                url: "/user/Get",
                method: "Post",
                dataType:'json',
                data: {
                    searchString:e.target.value
                },
                success: function (data) {

                    if (data != "" || data == e.target.value) {
                        if (e.target.value != "") {

                            spanforTag.value = spanforTag.value+" "+data;

                        }
                    }
                },

                error: function (err) {
                    console.log(err)
                }

            })

        })</script>



