@section Scripts{

    <script>let TagInput = document.getElementById("search-tag");
        let spanforTag = document.getElementById("Tags");


        const closeButtons = document.querySelectorAll(".spnX");

        closeButtons.forEach(c => {
            c.addEventListener("click", (e) => {
                console.log(e.target)
            })
        })

        TagInput.addEventListener("input", (e) => {

            $.ajax({
                url: "/user/Get",
                method: "Post",
                dataType: 'json',
                data: {
                    searchString: e.target.value
                },
                success: function (data) {
                    let value = e.target.value;

                    console.log(TagInput.nodeValue)
                    console.log(TagInput.value)

                    if (spanforTag.hasChildNodes()) {
                        let existed = document.querySelectorAll(".tagSing");

                        existed.forEach(e => {
                            if (e.textContent == data) {
                                data = '';
                            }

                        })
                    }

                    if (data != "" || data == value) {

                        if (value != "") {

                            const spnChild = document.createElement("span");
                            const spnX = document.createElement("span");
                            spnChild.classList.add("tagSing");
                            spnX.classList.add("spnX");


                            $(spanforTag).append(spnChild);
                            spnChild.innerText = data;

                            $(spnChild).append(spnX);

                            e.target.value = '';

                        }
                    }

                },

                error: function (err) {
                    console.log(err);
                }

            })
        })</script>
}


