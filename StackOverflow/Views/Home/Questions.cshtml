                        <span>per page</span>
                    </div>
                    <div class="container__main-content__sidebar pagination-left">
                        @if (ViewBag.Page > 1)
                        {
                            <a asp-controller="home" asp-action="questions" asp-route-page="@ViewBag.Page-1" class="pagination-item" title="Go to page 1">Prev</a>
                        }
                        @for (int i = 1; i <= ViewBag.TotalPage; i++)
                        {
                            <a class="pagination-item @(ViewBag.Page==i ? "is-selected" : "")" asp-controller="home" asp-action="questions" asp-route-page="@i">@i</a>
                        }
                        @if (ViewBag.TotalPage > 3)
                        {
                            <div class="clear">...</div>
                        }
                        @if (ViewBag.Page < ViewBag.TotalPage)
                        {
                            <a asp-controller="home" asp-action="questions" asp-route-page="@(ViewBag.Page+1)" class="pagination-item" title="Go to page 2">Next</a>
                        }
