                    <div class="container__main-content__questions--questions">
                        @foreach (Question question in Model)
                        {
                            <div class="flex fd-md-col">
                                <div class="stats">
                                    <div class="item">
                                        <span>0</span>
                                        <span>votes</span>
                                    </div>
                                    <div class="item">
                                        <span>0</span>
                                        <span>answers</span>
                                    </div>
                                    <div class="item">
                                        <span>2</span>
                                        <span>views</span>
                                    </div>
                                    <div class="item"></div>
                                    <div class="item"></div>
                                </div>
                                <div class="content">
                                    <h3>
                                        <a asp-route-id="@question.Id" asp-controller="home" asp-action="questiondetail">
                                            @question.Title
                                        </a>
                                    </h3>
                                    <div class="excerpt">
                                        @(question.Desc.Count()>200 ? question.Desc.Substring(0,200) : question.Desc)
                                    </div>
                                    <div class="meta">
                                        <div class="tags">
                                            @*<a href="">
                                                    <img src="https://i.stack.imgur.com/baRMT.png" alt="" />
                                                    amazon-web-services
                                                </a>
                                                <a href="">
                                                    <img src="https://i.stack.imgur.com/baRMT.png" alt="" />
                                                    aws-lambda
